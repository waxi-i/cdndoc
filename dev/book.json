{
    "title": "产品文档",
    "author": "Life",
    "description": "记录配置和一些插件的使用",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "Home": "https://baidu.com"
        }
    },
    "styles": {
        "website": "styles/website.css"
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "expandable-chapters-small",
        "copy-code-button",
        "versions-select"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "gitbookuse"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        }
    }
}