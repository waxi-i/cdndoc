{
    "title": "产品文档",
    "author": "Life",
    "description": "记录配置和一些插件的使用",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "Home": "https://baidu.com"
        }
    },
    "styles": {
        "website": "styles/website.css"
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "back-to-top-button",
        "expandable-chapters-small",
        "insert-logo",
        "toggle-chapters",
        "hide-element",
        "disqus",
        "github",
        "advanced-emoji",
        "splitter",
        "ga",
        "copy-code-button"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "gitbookuse"
        },
        "fontsettings": {
            "theme": "sepia",
            "family": "serif",
            "size": 1
        },
        "github": {
            "url": "https://github.com/"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "ga": {
            "token": "UA-XXXX-Y"
        }
    }
}